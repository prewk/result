{
    "name": "prewk/result",
    "description": "Result object for PHP inspired by Rust",
    "type": "library",
    "license": "(MIT and Apache-2.0)",
    "autoload": {
        "psr-4": {
            "Prewk\\": "src/"
        }
    },
    "scripts": {
        "test": "phpspec run && psalm"
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Oskar Thornblad",
            "email": "oskar.thornblad@gmail.com"
        }
    ],
    "require": {
        "prewk/option": ">= 1.3.0",
        "php": ">=7.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.4.3",
        "leanphp/phpspec-code-coverage": "^3.1.1",
        "php-coveralls/php-coveralls": "^2.4.3",
        "vimeo/psalm": "^3.2.12"
    }
}
