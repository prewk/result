{
    "name": "prewk/result",
    "description": "Result object for PHP inspired by Rust",
    "type": "library",
    "license": "(MIT and Apache-2.0)",
    "autoload": {
        "psr-4": {
            "Prewk\\": "src/"
        }
    },
    "scripts": {
        "test": "phpspec run && psalm"
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Oskar Thornblad",
            "email": "oskar.thornblad@gmail.com"
        }
    ],
    "require": {
        "prewk/option": ">= 1.3.0",
        "php": ">=7.3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0.1",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0.0",
        "phpunit/php-code-coverage": "^9.2.5",
        "phpunit/php-file-iterator": "^3.0.5",
        "php-coveralls/php-coveralls": "^2.4.3",
        "guzzlehttp/guzzle": "^7.3.0",
        "vimeo/psalm": "^4.6.4"
    }
}
